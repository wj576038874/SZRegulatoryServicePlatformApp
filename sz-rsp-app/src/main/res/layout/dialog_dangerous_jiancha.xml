<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_gravity="center"
    android:background="@android:color/transparent"
    android:focusable="true"
    android:focusableInTouchMode="true">

    <LinearLayout
        android:id="@+id/msg_Relativelayout_msg_content"
        android:layout_width="wrap_content"
        android:layout_height="330dp"
        android:background="@drawable/message_bg"
        android:orientation="vertical">

        <RelativeLayout
            android:id="@+id/msg_LinearLayout_msg"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:layout_marginBottom="5dp"
            android:background="@drawable/message_title_bg"
            android:paddingLeft="10dp">

            <TextView
                android:id="@+id/zhengshureason_tv_title"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="5dp"
                android:clickable="true"
                android:gravity="center"
                android:text="检查代码"
                android:textColor="#333"
                android:textSize="15sp" />

            <ImageButton
                android:id="@+id/but_close"
                android:layout_width="45dp"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:background="@drawable/table_pop_cancel_btn"
                android:src="@mipmap/network_error_close" />
        </RelativeLayout>

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scrollbars="none">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:background="@color/tablebackground"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        android:layout_marginLeft="30dp"
                        android:gravity="center_vertical"
                        android:text="1.船舶证书、文书、资料 "
                        android:textColor="#ff333333"
                        android:textSize="16sp" />
                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.1dp"
                    android:layout_marginLeft="30dp"
                    android:background="@color/view_line_color" />

                <CheckBox
                    android:id="@+id/check_1_1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="10dp"
                    android:minHeight="30dp"
                    android:paddingLeft="10dp"
                    android:text="1.1船舶载运危险货物符合证明"
                    android:textSize="15sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.1dp"
                    android:layout_marginLeft="30dp"
                    android:background="@color/view_line_color" />

                <CheckBox
                    android:id="@+id/check_1_2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="10dp"
                    android:minHeight="30dp"
                    android:paddingLeft="10dp"
                    android:text="1.2危险货物运输单证、集装箱装箱证明书"
                    android:textSize="15sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.1dp"
                    android:layout_marginLeft="30dp"
                    android:background="@color/view_line_color" />

                <CheckBox
                    android:id="@+id/check_1_3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="10dp"
                    android:minHeight="30dp"
                    android:paddingLeft="10dp"
                    android:text="1.3船舶载运危险货物/污染危害性货物申报单"
                    android:textSize="15sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.1dp"
                    android:layout_marginLeft="30dp"
                    android:background="@color/view_line_color" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        android:layout_marginLeft="30dp"
                        android:gravity="center_vertical"
                        android:text="危险货物清单"
                        android:textColor="#ff333333"
                        android:textSize="16sp" />
                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.1dp"
                    android:layout_marginLeft="30dp"
                    android:background="@color/view_line_color" />

                <CheckBox
                    android:id="@+id/check_1_4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="10dp"
                    android:minHeight="30dp"
                    android:paddingLeft="10dp"
                    android:text="1.4船舶的总积载图"
                    android:textSize="15sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.1dp"
                    android:layout_marginLeft="30dp"
                    android:background="@color/view_line_color" />

                <CheckBox
                    android:id="@+id/check_1_5"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="10dp"
                    android:minHeight="30dp"
                    android:paddingLeft="10dp"
                    android:text="1.5积载与船舶载运危险货物符合证明符合性"
                    android:textSize="15sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.1dp"
                    android:layout_marginLeft="30dp"
                    android:background="@color/view_line_color" />

                <CheckBox
                    android:id="@+id/check_1_6"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="10dp"
                    android:minHeight="30dp"
                    android:paddingLeft="10dp"
                    android:text="1.6积载与《危规的符合性》"
                    android:textSize="15sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.1dp"
                    android:layout_marginLeft="30dp"
                    android:background="@color/view_line_color" />

                <CheckBox
                    android:id="@+id/check_1_7"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="10dp"
                    android:minHeight="30dp"
                    android:paddingLeft="10dp"
                    android:text="1.7隔离与《危规》符合性"
                    android:textSize="15sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.1dp"
                    android:layout_marginLeft="30dp"
                    android:background="@color/view_line_color" />

                <CheckBox
                    android:id="@+id/check_1_8"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="10dp"
                    android:minHeight="30dp"
                    android:paddingLeft="10dp"
                    android:text="1.8船舶安全营运和防污染管理体系文书配备"
                    android:textSize="15sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.1dp"
                    android:layout_marginLeft="30dp"
                    android:background="@color/view_line_color" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:background="@color/tablebackground"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        android:layout_marginLeft="30dp"
                        android:gravity="center_vertical"
                        android:text="2.船舶结构、设备"
                        android:textColor="#ff333333"
                        android:textSize="16sp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        android:layout_marginLeft="30dp"
                        android:gravity="center_vertical"
                        android:text="供水布置"
                        android:textColor="#ff333333"
                        android:textSize="16sp" />
                </LinearLayout>

                <CheckBox
                    android:id="@+id/check_2_1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="10dp"
                    android:minHeight="30dp"
                    android:paddingLeft="10dp"
                    android:text="2.1着火源"
                    android:textSize="15sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.1dp"
                    android:layout_marginLeft="30dp"
                    android:background="@color/view_line_color" />

                <CheckBox
                    android:id="@+id/check_2_2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="10dp"
                    android:minHeight="30dp"
                    android:paddingLeft="10dp"
                    android:text="2.2探测系统"
                    android:textSize="15sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.1dp"
                    android:layout_marginLeft="30dp"
                    android:background="@color/view_line_color" />

                <CheckBox
                    android:id="@+id/check_2_3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="10dp"
                    android:minHeight="30dp"
                    android:paddingLeft="10dp"
                    android:text="2.3对货舱货物处所的通风是否符合相关要求"
                    android:textSize="15sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.1dp"
                    android:layout_marginLeft="30dp"
                    android:background="@color/view_line_color" />

                <CheckBox
                    android:id="@+id/check_2_4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="10dp"
                    android:minHeight="30dp"
                    android:paddingLeft="10dp"
                    android:text="2.4舱底泵系统"
                    android:textSize="15sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.1dp"
                    android:layout_marginLeft="30dp"
                    android:background="@color/view_line_color" />

                <CheckBox
                    android:id="@+id/check_2_5"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="10dp"
                    android:minHeight="30dp"
                    android:paddingLeft="10dp"
                    android:text="2.5人员的保护"
                    android:textSize="15sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.1dp"
                    android:layout_marginLeft="30dp"
                    android:background="@color/view_line_color" />

                <CheckBox
                    android:id="@+id/check_2_6"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="10dp"
                    android:minHeight="30dp"
                    android:paddingLeft="10dp"
                    android:text="2.6手提式灭火器"
                    android:textSize="15sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.1dp"
                    android:layout_marginLeft="30dp"
                    android:background="@color/view_line_color" />

                <CheckBox
                    android:id="@+id/check_2_7"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="10dp"
                    android:minHeight="30dp"
                    android:paddingLeft="10dp"
                    android:text="2.7机器处所界面的隔热"
                    android:textSize="15sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.1dp"
                    android:layout_marginLeft="30dp"
                    android:background="@color/view_line_color" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:background="@color/tablebackground"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        android:layout_marginLeft="30dp"
                        android:gravity="center_vertical"
                        android:text="3.危险货物集装箱的箱体、标志、系固的检查"
                        android:textColor="#ff333333"
                        android:textSize="16sp" />
                </LinearLayout>

                <CheckBox
                    android:id="@+id/check_3_1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="10dp"
                    android:minHeight="30dp"
                    android:paddingLeft="10dp"
                    android:text="3.1  悬挂或显示装运危险货物信号"
                    android:textSize="15sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.1dp"
                    android:layout_marginLeft="30dp"
                    android:background="@color/view_line_color" />

                <CheckBox
                    android:id="@+id/check_3_2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="10dp"
                    android:minHeight="30dp"
                    android:paddingLeft="10dp"
                    android:text="3.2  集装箱安全合格牌照"
                    android:textSize="15sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.1dp"
                    android:layout_marginLeft="30dp"
                    android:background="@color/view_line_color" />

                <CheckBox
                    android:id="@+id/check_3_3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="10dp"
                    android:minHeight="30dp"
                    android:paddingLeft="10dp"
                    android:text="3.3  集装箱箱体的检查"
                    android:textSize="15sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.1dp"
                    android:layout_marginLeft="30dp"
                    android:background="@color/view_line_color" />

                <CheckBox
                    android:id="@+id/check_3_4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="10dp"
                    android:minHeight="30dp"
                    android:paddingLeft="10dp"
                    android:text="3.4  标记标志和标牌"
                    android:textSize="15sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.1dp"
                    android:layout_marginLeft="30dp"
                    android:background="@color/view_line_color" />

                <CheckBox
                    android:id="@+id/check_3_5"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="10dp"
                    android:minHeight="30dp"
                    android:paddingLeft="10dp"
                    android:text="3.5  集装箱系固设备"
                    android:textSize="15sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.1dp"
                    android:layout_marginLeft="30dp"
                    android:background="@color/view_line_color" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:background="@color/tablebackground"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        android:layout_marginLeft="30dp"
                        android:gravity="center_vertical"
                        android:text="4.危险货物货况"
                        android:textColor="#ff333333"
                        android:textSize="16sp" />
                </LinearLayout>

                <CheckBox
                    android:id="@+id/check_4_1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="10dp"
                    android:minHeight="30dp"
                    android:paddingLeft="10dp"
                    android:text="4.1  危险货物包装"
                    android:textSize="15sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.1dp"
                    android:layout_marginLeft="30dp"
                    android:background="@color/view_line_color" />

                <CheckBox
                    android:id="@+id/check_4_2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="10dp"
                    android:minHeight="30dp"
                    android:paddingLeft="10dp"
                    android:text="4.2  堆码"
                    android:textSize="15sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.1dp"
                    android:layout_marginLeft="30dp"
                    android:background="@color/view_line_color" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:background="@color/tablebackground"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        android:layout_marginLeft="30dp"
                        android:gravity="center_vertical"
                        android:text="5.人员能力"
                        android:textColor="#ff333333"
                        android:textSize="16sp" />
                </LinearLayout>

                <CheckBox
                    android:id="@+id/check_5_1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="10dp"
                    android:minHeight="30dp"
                    android:paddingLeft="10dp"
                    android:text="5.1大副或船长是否熟悉其职责"
                    android:textSize="15sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.1dp"
                    android:layout_marginLeft="30dp"
                    android:background="@color/view_line_color" />

                <CheckBox
                    android:id="@+id/check_5_2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="10dp"
                    android:minHeight="30dp"
                    android:paddingLeft="10dp"
                    android:text="5.2大副或船长是否熟悉货物具有的危险性以及应采取的安全措施"
                    android:textSize="15sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.1dp"
                    android:layout_marginLeft="30dp"
                    android:background="@color/view_line_color" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:background="@color/tablebackground"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        android:layout_marginLeft="30dp"
                        android:gravity="center_vertical"
                        android:text="6.应急反应"
                        android:textColor="#ff333333"
                        android:textSize="16sp" />
                </LinearLayout>

                <CheckBox
                    android:id="@+id/check_6_1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="10dp"
                    android:minHeight="30dp"
                    android:paddingLeft="10dp"
                    android:text="6.1  事故应急反应及报告程序"
                    android:textSize="15sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.1dp"
                    android:layout_marginLeft="30dp"
                    android:background="@color/view_line_color" />

                <CheckBox
                    android:id="@+id/check_6_2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="10dp"
                    android:minHeight="30dp"
                    android:paddingLeft="10dp"
                    android:text="6.2  应急反应指南和医疗急救指南"
                    android:textSize="15sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.1dp"
                    android:layout_marginLeft="30dp"
                    android:background="@color/view_line_color" />

                <CheckBox
                    android:id="@+id/check_6_3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="10dp"
                    android:minHeight="30dp"
                    android:paddingLeft="10dp"
                    android:text="6.3  应急处理器材和防护用品"
                    android:textSize="15sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.1dp"
                    android:layout_marginLeft="30dp"
                    android:background="@color/view_line_color" />

                <CheckBox
                    android:id="@+id/check_6_4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="10dp"
                    android:minHeight="30dp"
                    android:paddingLeft="10dp"
                    android:text="6.4  船员熟悉船舶所载运的危险货物的危险特性和发生事故时的应急措施"
                    android:textSize="15sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.1dp"
                    android:layout_marginLeft="30dp"
                    android:background="@color/view_line_color" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:background="@color/tablebackground"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        android:layout_marginLeft="30dp"
                        android:gravity="center_vertical"
                        android:text="7.其它"
                        android:textColor="#ff333333"
                        android:textSize="16sp" />
                </LinearLayout>

                <CheckBox
                    android:id="@+id/check_7_1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="10dp"
                    android:minHeight="30dp"
                    android:paddingLeft="10dp"
                    android:text="7.1 其他项目（用文字说明）"
                    android:textSize="15sp" />

                <EditText
                    android:id="@+id/edit_opinion"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:layout_marginBottom="10dp"
                    android:layout_marginLeft="50dp"
                    android:layout_marginRight="10dp"
                    android:background="#edeef2"
                    android:enabled="false"
                    android:gravity="top"
                    android:hint="请填写其他检查项目"
                    android:padding="5dp"
                    android:textColor="#333333"
                    android:textColorHint="#999999"
                    android:textSize="15sp" />
            </LinearLayout>
        </ScrollView>
    </LinearLayout>


    <RelativeLayout
        android:id="@+id/msg_rl_operate"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_below="@id/msg_Relativelayout_msg_content"
        android:layout_marginTop="5dp"
        android:background="@drawable/message_bg"
        android:visibility="visible">

        <TextView
            android:id="@+id/but_save"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="15dp"
            android:clickable="true"
            android:gravity="center"
